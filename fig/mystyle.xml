<?xml version="1.0" encoding="utf-8"?>
<Map background-color="#000000">
	<Style name="target_lines">
		<Rule>
			<LineSymbolizer stroke="#00FF7F" stroke-width="5" stroke-linejoin="round"
				stroke-linecap="round" stroke-opacity="0.7"/>
		</Rule>
	</Style>

	<Style name="background_lines">
		<Rule>
			<LineSymbolizer stroke="#D3D3D3" stroke-width="5" stroke-linejoin="round"
				stroke-linecap="round" stroke-opacity="0.7"/>
		</Rule>
	</Style>
    
    <Style name="background_points">
		<Rule>
            <PointSymbolizer file= "./red_small.png"/>
		</Rule>
	</Style>
    
    <Style name="target_points">
		<Rule>
			<PointSymbolizer file= "./path_node.png"/>
		</Rule>
	</Style>

    <Style name="root_points">
		<Rule>
			<PointSymbolizer file= "./yellow_small.png" />
		</Rule>
	</Style>
	<Layer name="target_points">
		<StyleName>target_points</StyleName>
			<Datasource>                    
				<Parameter name="type">shape</Parameter>
                    <Parameter name="file">target_points.shp</Parameter>
			</Datasource>
	</Layer>
	<Layer name="root_points">
		<StyleName>root_points</StyleName>
			<Datasource>                    
				<Parameter name="type">shape</Parameter>
                    <Parameter name="file">root_points.shp</Parameter>
			</Datasource>
	</Layer>
	<Layer name="target_lines">
		<StyleName>target_lines</StyleName>
			<Datasource>                    
				<Parameter name="type">shape</Parameter>
                    <Parameter name="file">target_lines.shp</Parameter>
			</Datasource>
	</Layer>
	<Layer name="background_points">
		<StyleName>background_points</StyleName>
			<Datasource>                    
				<Parameter name="type">shape</Parameter>
                    <Parameter name="file">background_points.shp</Parameter>
			</Datasource>
	</Layer>
</Map>